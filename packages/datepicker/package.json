{
  "name": "@mathieustan/vue-datepicker",
  "description": "A clean datepicker made with VueJs",
  "version": "0.2.11",
  "author": "Mathieu Stanowski <mathieu.stan@gmail.com>",
  "keywords": [
    "vue",
    "vuejs",
    "javascript",
    "vue-component",
    "date",
    "month",
    "quarter",
    "picker",
    "datepicker",
    "monthpicker",
    "quarterpicker"
  ],
  "license": "MIT",
  "files": [
    "dist/*.js",
    "dist/*.css",
    "src/*"
  ],
  "main": "dist/vue-datepicker.umd.js",
  "module": "dist/vue-datepicker.esm.js",
  "unpkg": "dist/vue-datepicker.min.js",
  "repository": "git+https://github.com/mathieustan/vue-datepicker.git",
  "bugs": {
    "url": "https://github.com/mathieustan/vue-datepicker/issues"
  },
  "homepage": "https://vue-datepicker.netlify.com/",
  "scripts": {
    "dev": "yarn build:es -- --watch",
    "build": "yarn build:browser && yarn build:es && yarn build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "send:coverage": "cat ./tests/coverage/lcov.info | codacy-coverage",
    "lint": "eslint --ext .js,.vue src tests/unit/* build/*",
    "lint:fix": "yarn lint --fix",
    "test:client": "vue-cli-service test:unit",
    "prepublishOnly": "yarn lint:fix && yarn test:client"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.1",
    "dayjs": "1.8.24"
  },
  "peerDependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-define": "^2.0.0",
    "codacy-coverage": "^3.4.0",
    "core-js": "^2.6.5",
    "cssnano": "^4.1.10",
    "mockdate": "^2.0.2",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^5.0.2",
    "rollup": "^1.14.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-css-porter": "^0.1.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "rollup-plugin-vue": "4.2.0",
    "sass": "^1.26.5",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "directories": {
    "test": "tests"
  }
}
